.prize-section {
    background: #222;
    padding: 30px;
    margin: 50px auto;
    width: 90%;
    max-width: 600px;
    border-radius: 15px;
    color: white;
    text-align: center;
}

.prize-section ul {
    list-style: none;
    padding: 0;
}

.prize-section li {
    margin-bottom: 15px;
    font-size: 1.2em;
}
